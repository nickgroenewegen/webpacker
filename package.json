{
  "name": "milli-vanilli",
  "version": "1.0.0",
  "description": "Webpack boilerplate for writing javascript applications",
  "main": "index.js",
  "author": "Nick Groenewegen",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config webpack/webpack.development.js",
    "test": "./node_modules/.bin/jest --no-cache",
    "test:e2e": "./node_modules/.bin/cypress open",
    "lint": "./node_modules/.bin/eslint --ext .js jest webpack src",
    "typecheck": "./node_modules/.bin/flow",
    "build": "./node_modules/.bin/webpack --config webpack/webpack.production.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-flow": "^6.23.0",
    "clean-webpack-plugin": "^3.0.0",
    "cypress": "^3.3.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.14.0",
    "flow-bin": "^0.101.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5"
  }
